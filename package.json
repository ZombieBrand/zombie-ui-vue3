{
  "name": "zombie-ui-vue3",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/zombie-ui.umd.cjs",
  "module": "./dist/zombie-ui.js",
  "exports": {
    ".": {
      "import": "./dist/zombie-ui.js",
      "require": "./dist/zombie-ui.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "docs:dev": "vitepress dev docs --host",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "lint": "eslint --fix --ext .ts,.vue,.tsx src",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.vue\" \"src/**/*.tsx\"",
    "test": "vitest",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "vue": "^3.2.45",
    "vue-router": "4"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@iconify-json/mdi": "^1.1.48",
    "@iconify/vue": "^4.1.0",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/compiler-sfc": "^3.2.47",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/test-utils": "^2.0.2",
    "autoprefixer": "^10.4.13",
    "cssnano": "^5.1.15",
    "eslint": "^8.35.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "happy-dom": "^6.0.4",
    "husky": "^8.0.3",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.3",
    "sass": "^1.58.3",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.3",
    "unplugin-icons": "^0.15.3",
    "unplugin-vue-components": "^0.24.0",
    "vite": "^4.1.0",
    "vitepress": "1.0.0-alpha.49",
    "vitest": "^0.21.1",
    "vue-tsc": "^1.0.24"
  }
}